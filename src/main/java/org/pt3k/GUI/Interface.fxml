<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainVBox" prefHeight="375.0" prefWidth="743.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pt3k.AppController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox fx:id="HBox">
         <children>
            <VBox minWidth="145.0" prefHeight="2000.0" prefWidth="160.0">
               <children>
                  <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#generateImage" prefHeight="25.0" prefWidth="281.0" text="GENERATE" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Label text="Resolution">
                     <VBox.margin>
                        <Insets left="8.0" right="5.0" />
                     </VBox.margin>
                  </Label>
                  <HBox>
                     <children>
                        <TextField fx:id="resolutionX" text="200">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="2.0" />
                           </HBox.margin>
                        </TextField>
                        <Label text="x">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField fx:id="resolutionY" text="100">
                           <HBox.margin>
                              <Insets bottom="5.0" left="2.0" right="5.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <Label layoutX="10.0" layoutY="45.0" text="Samples">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="samples" text="10" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <Label layoutX="10.0" layoutY="87.0" text="Depth">
                     <VBox.margin>
                        <Insets left="8.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="depth" layoutX="10.0" layoutY="104.0" prefHeight="25.0" prefWidth="104.0" text="10">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <ImageView fx:id="imageView" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
      <Pane prefHeight="200.0" prefWidth="200.0" />
      <HBox>
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Label fx:id="statusLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT">
               <HBox.margin>
                  <Insets bottom="3.0" left="5.0" right="5.0" top="3.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</VBox>
